{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": "dotenv -e .env.local tsx src/db/seed.ts"
  },
  "scripts": {
    "clean": "rimraf --glob node_modules && rimraf --glob .next",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "dotenv -e .env.local prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- node --no-warnings=ExperimentalWarning --loader ts-node/esm src/db/seed.ts",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev --name",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "npm run lint && npm run typecheck && npm run format:write",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
